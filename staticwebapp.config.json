{
  "platform": {
    "apiRuntime": "node:20"
  },
  "routes": [
    {
      "route": "/sw.js",
      "headers": {
        "Service-Worker-Allowed": "/",
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/workbox-*.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/favicon.ico",
      "serve": "/favicon.ico",
      "statusCode": 200
    },
    {
      "route": "/robots.txt",
      "serve": "/robots.txt",
      "statusCode": 200
    },
    {
      "route": "/llms.txt",
      "serve": "/llms.txt",
      "statusCode": 200
    },
    {
      "route": "/sitemap.xml",
      "serve": "/sitemap.xml",
      "statusCode": 200
    },
    {
      "route": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
